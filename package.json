    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "concurrently": "^7.6.0",
    "eslint": "^8.42.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.3",
    "typescript": "^4.9.5"
  }
